{
    "name": "ray/aura-sql-module",
    "keywords": [
        "Ray module",
        "Aura Sql",
        "PDO"
    ],
    "description": "aura/sql module for Ray.Di",
    "license": "MIT",
    "require": {
        "php": "~5.6|~7.0",
        "ray/di": "^2.5.0",
        "aura/sql": "^2.5.1",
        "aura/sqlquery": "^2.7.1",
        "pagerfanta/pagerfanta": "^1.0.4",
        "rize/uri-template": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Ray\\AuraSqlModule\\": "src/"
        },
        "files": [
            "src-files/uri_template.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ray\\AuraSqlModule\\": [
                "tests/",
                "tests/Fake/"
            ]
        }
    }
}
